{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "sourceMap": false,
    "outDir": "dist/js",
    "noEmitOnError": true,
    "jsx": "react-jsx",
    "typeRoots": ["node_modules/@types"],
    "strictPropertyInitialization": false
  },
  "include": ["./src/**/*.ts", "./src/**/*.tsx", "src/App.stories.tsx", "./cypress/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
